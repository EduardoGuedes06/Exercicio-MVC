@{
    ViewData["Title"] = "Ranking";
}
<div class="container">
    <div class="row">
        <div class="col">
            <div class="text-center">
                <h3>Ranking</h3>
            </div>
            <div class="table-responsive" style="max-height: 300px;">
                <table class="table table-striped table-scroll" id="RankingTable">
                    <thead>
                        <tr>
                            <th>Jogador</th>
                            <th>Ganho %</th>
                            <th>Classificação</th>
                            <th>Modalidade</th>
                            <th>Player</th>
                            <th>Resultado</th>
                        </tr>
                    </thead>
                    <tbody id="RankingTableBody">
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
